<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    RewriteRule ^index\.html$ - [L]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . /dist/index.html [L]
  </IfModule>
  
  AddType application/javascript js
  AddType text/css css
  
  <FilesMatch "\.js$">
      AddType application/javascript js
  </FilesMatch>
  
  <FilesMatch "\.css$">
      AddType text/css css
  </FilesMatch>
  